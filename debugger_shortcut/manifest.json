{
    "manifest_version": 3,
    "name": "debugger_shortcut",
    "version": "1.0",
    "description": "Listen for the F2 key and display a UI prompt.",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  